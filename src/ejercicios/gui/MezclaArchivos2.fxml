      </Label>
      <Button fx:id="archivo1" mnemonicParsing="false" onAction="#archivoOrigen1" text="Selecciona archivo 1" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
      <Button fx:id="archivo2" mnemonicParsing="false" onAction="#archivoOrigen2" text="Selecciona archivo 2" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
